{
  "name": "sneedznab",
  "version": "1.2.5",
  "description": "Newznab/torznab indexer for the Sneedex",
  "author": "sw-carlos-cristobal",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "local": "node -r dotenv/config --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "dev": "nodemon",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn lint --fix && prettier --write src/",
    "curl": "curl -v http://localhost:3000/api",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/sneedznab.git"
  },
  "bugs": {
    "url": "https://github.com/TurboCheetah/sneedznab/issues"
  },
  "homepage": "https://github.com/TurboCheetah/sneedznab#readme",
  "dependencies": {
    "@hono/node-server": "^1.0.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/parser": "^5.59.9",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.1.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fuse.js": "6.6.2",
    "hono": "3.2.4",
    "lru-cache": "^9.1.2",
    "node": "20",
    "picocolors": "1.0.0",
    "prettier": "^2.8.8",
    "redis": "^4.6.7",
    "ts-node": "^10.9.1",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/eslint": "8.40.0",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "bun-types": "0.6.7",
    "eslint": "^8.42.0",
    "nodemon": "2.0.22",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "arrowParens": "always",
    "trailingComma": "none",
    "endOfLine": "auto"
  }
}
