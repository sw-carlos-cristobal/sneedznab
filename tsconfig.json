{
  "compilerOptions": {
    "lib": [
      "ES6",
      "DOM"
    ],
    "module": "ESNext",
    "target": "ES6",
    "types": [
      "node"
    ],
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "outDir": "./dist",
    "paths": {
      "#/*": [
        "*"
      ],
      "#interfaces/*": [
        "interfaces/*"
      ],
      "#providers/*": [
        "providers/*"
      ],
      "#routes/*": [
        "routes/*"
      ],
      "#utils/*": [
        "utils/*"
      ],
    },
  }
}